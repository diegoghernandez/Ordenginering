{
	"extends": "astro/tsconfigs/strict",
	"include": ["src", "src/test/setup-vitest.ts", "src/types.d.ts"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"target": "ES2023",
		"lib": ["es5", "dom", "DOM.Iterable"],
		"types": ["astro/client"],
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
